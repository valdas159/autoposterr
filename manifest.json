{
  "name": "AUTO POST MULTIPLE FACEBOOK GROUPS V4 2021 - JERA ",
  "description": "BY JERA",
  "version": "4.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
